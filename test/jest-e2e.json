{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@bilr/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@bilr/logger": "<rootDir>/../libs/logger/src"
  }
}
